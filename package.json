{
  "name": "electron-clash-lite",
  "version": "1.0.0",
  "main": "dist/electron/main/index.js",
  "author": "SharerMax",
  "license": "MIT",
  "productName": "ClashLite",
  "scripts": {
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:dir": "vue-tsc --noEmit && vite build && electron-builder --dir",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "packageManager": "pnpm@7.3.0",
  "devDependencies": {
    "@playwright/test": "1.22.2",
    "@types/js-yaml": "4.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "@vicons/carbon": "0.12.0",
    "@vitejs/plugin-vue": "2.3.3",
    "chart.js": "3.8.0",
    "electron": "19.0.6",
    "electron-builder": "23.1.0",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-vue": "9.1.1",
    "get-port": "6.1.2",
    "js-yaml": "4.1.0",
    "naive-ui": "2.30.6",
    "nano-staged": "0.8.0",
    "simple-git-hooks": "2.8.0",
    "typescript": "4.7.4",
    "unocss": "0.41.0",
    "vfonts": "0.0.3",
    "vite": "2.9.12",
    "vite-plugin-electron": "0.4.9",
    "vite-plugin-resolve": "2.1.2",
    "vue": "3.2.37",
    "vue-chartjs": "4.1.1",
    "vue-router": "4.0.16",
    "vue-tsc": "0.38.2"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
