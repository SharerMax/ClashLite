{
  "name": "electron-clash-lite",
  "version": "1.0.0",
  "packageManager": "pnpm@8.1.0",
  "author": "SharerMax",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "main": "dist/electron/main/index.js",
  "productName": "ClashLite",
  "engines": {
    "node": ">=18.11.0 <19"
  },
  "scripts": {
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "dev": "vite",
    "debug": "vite --mode debug",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:dir": "vue-tsc --noEmit && vite build && electron-builder --dir",
    "test": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "githook": "simple-git-hooks",
    "build:all": "turbo run build",
    "dev:all": "turbo run dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/prompt-cli": "^17.6.6",
    "@playwright/test": "^1.35.1",
    "@tsconfig/node18": "^18.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.16.19",
    "@types/node-schedule": "^2.1.0",
    "@types/url-parse": "^1.4.8",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.2",
    "electron": "25.2.0",
    "electron-builder": "^24.4.0",
    "electron-store": "^8.1.0",
    "eslint": "^8.44.0",
    "get-port": "^7.0.0",
    "internal-ip": "^8.0.0",
    "js-base64": "^3.7.5",
    "js-yaml": "^4.1.0",
    "naive-ui": "^2.34.4",
    "nano-staged": "^0.8.0",
    "node-schedule": "^2.1.1",
    "punycode-esm": "^1.0.6",
    "share": "workspace:*",
    "simple-git-hooks": "^2.8.1",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
    "url-parse": "^1.5.10",
    "vfonts": "^0.0.3",
    "vite": "^4.4.2",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-resolve": "^2.3.0",
    "vite-plugin-vue-devtools": "^0.4.14",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.0",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.4"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  }
}
