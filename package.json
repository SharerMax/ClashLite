{
  "name": "electron-clash-lite",
  "version": "1.0.0",
  "packageManager": "pnpm@7.16.1",
  "author": "SharerMax",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "main": "dist/electron/main/index.js",
  "productName": "ClashLite",
  "engines": {
    "node": ">=18.11.0 <19"
  },
  "scripts": {
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "dev": "vite",
    "debug": "vite --mode debug",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:dir": "vue-tsc --noEmit && vite build && electron-builder --dir",
    "test": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "githook": "simple-git-hooks",
    "build:all": "turbo run build",
    "dev:all": "turbo run dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.31.0",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@commitlint/prompt-cli": "^17.2.0",
    "@playwright/test": "1.28.0",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.9",
    "@types/node-schedule": "2.1.0",
    "@types/url-parse": "1.4.8",
    "@vicons/carbon": "0.12.0",
    "@vitejs/plugin-vue": "3.2.0",
    "@vueuse/core": "9.5.0",
    "axios": "1.1.3",
    "dayjs": "1.11.6",
    "echarts": "5.4.0",
    "electron": "21.3.0",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "3.2.0",
    "electron-store": "8.1.0",
    "eslint": "8.27.0",
    "get-port": "6.1.2",
    "internal-ip": "7.0.0",
    "js-base64": "3.7.3",
    "js-yaml": "4.1.0",
    "naive-ui": "2.33.5",
    "nano-staged": "0.8.0",
    "node-schedule": "2.1.0",
    "punycode-esm": "1.0.6",
    "share": "workspace:*",
    "simple-git-hooks": "2.8.1",
    "turbo": "^1.6.3",
    "typescript": "4.9.3",
    "unocss": "0.46.5",
    "url-parse": "1.5.10",
    "vfonts": "0.0.3",
    "vite": "3.2.4",
    "vite-plugin-electron": "0.10.4",
    "vite-plugin-resolve": "2.3.0",
    "vitest": "0.25.2",
    "vue": "3.2.45",
    "vue-echarts": "6.2.3",
    "vue-router": "4.1.6",
    "vue-tsc": "1.0.9"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  }
}
